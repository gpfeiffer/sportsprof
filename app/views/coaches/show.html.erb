<h3>Coaches</h3>
<h2><%= @coach.name %></h2>

<p>
  <b>Gender:</b>
  <%= @coach.gender %>
</p>

<p>
  <b>Description:</b>
  <%= @coach.description %>
</p>


<% if can? :edit, @coach %>
  <%= link_to 'Edit', edit_coach_path(@coach) %> |
<% end %>
<% if can? :destroy, @coach %>
  <%= link_to 'Delete', @coach , :confirm => 'Are you sure?', :method => :delete %> |
<% end %>
<%= link_to 'Back', coaches_path %>
